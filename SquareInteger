package Homework13;

public class SquareInteger {

	public static void main(String[] args) {

		System.out.println(findNumSqare(3, 9));
		System.out.println(findNumSqare(17, 24));

	}

	private static int findNumSqare(int a, int b) {

		int count = 0;
		for (int i = a; i <= b; i++) {

			if (Math.pow(i, 2) >= a && Math.pow(i, 2) <= b)
				count++;
		}
		return count;
	}

}
